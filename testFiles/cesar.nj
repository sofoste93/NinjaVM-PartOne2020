
void main(){
    local Integer result;  
    
    result=print_n_times(3,25);
    writeInteger(result);
    writeCharacter('\n');
}

Integer print_n_times(Integer n, Integer p){
    local Integer i;
    local Integer result;

    if(n<0){
        return -1;
    }
    
    i=0;
    while(i!=n){
        writeInteger(p);
        writeCharacter('\n');
        i=i+1;
    }

    result=n+p;
    
    return result;
}
